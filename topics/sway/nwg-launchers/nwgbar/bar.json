[
  {
    "name": "Lock screen",
    "exec": "swaylock -C ~/.config/swaylock/config",
    "icon": "/home/bryan/.config/nwg-launchers/nwgbar/images/lock.svg"
  },
  {
    "name": "Suspend",
    "exec": "systemctl suspend;swaylock -C ~/.config/swaylock/config",
    "icon": "/home/bryan/.config/nwg-launchers/nwgbar/images/suspend.svg"
  },
  {
    "name": "Logout",
    "exec": "loginctl terminate-user $USER",
    "icon": "/home/bryan/.config/nwg-launchers/nwgbar/images/logout.svg"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "/home/bryan/.config/nwg-launchers/nwgbar/images/reboot.svg"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "/home/bryan/.config/nwg-launchers/nwgbar/images/shutdown.svg"
  }
]
